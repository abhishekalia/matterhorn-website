{
  "installCommand": "npm install",
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/contact.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/marshmma",
      "destination": "https://marsh-mma-landing.replit.app"
    },
    {
      "source": "/marshmma/:path*",
      "destination": "https://marsh-mma-landing.replit.app/:path*"
    }
  ]
}
