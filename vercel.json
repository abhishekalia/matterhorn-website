{
  "installCommand": "npm install",
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/contact.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/broker-application.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/marshmma",
      "destination": "https://marsh-mma-landing.replit.app/",
      "permanent": false
    }
  ]
}
